package Ordenacao;

public class Pessoa implements Comparable<T>{
	private String nome;
	private double altura;
	private int idade;
	
	public String getNome() {
		return nome;
	}
	public double getAltura() {
		return altura;
	}
	public int getIdade() {
		return idade;
	}
	
	
	
	public Pessoa(String nome, int idade, double altura) {
		this.nome = nome;
		this.altura = altura;
		this.idade = idade;
	}
	
	@Override
	public String toString() {
		return " " + nome + " " + altura + " " + idade ;
	}
	
	

}
