package Ordenacao;

public class Pessoa {
	private String nome;
	private double altura;
	private int idade;
	
	public String getNome() {
		return nome;
	}
	public double getAltura() {
		return altura;
	}
	public int getIdade() {
		return idade;
	}
	
	public Pessoa(String nome, int idade, double altura){
		this.nome = nome;
		this.idade = idade;
		this.altura = altura;
	}
}
